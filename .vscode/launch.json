{
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/pipex",
            "args": [
                "infile",
                "cat",
                "sleep",
                "/dev/stdout"
